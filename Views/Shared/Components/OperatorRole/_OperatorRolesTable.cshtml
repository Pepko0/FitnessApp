@model IEnumerable<FitnessApp.Models.OperatorRole>

<table class="table table-striped align-middle">
    <thead>
        <tr>
            <th style="width:80px;">Id</th>
            <th>Nazwa roli</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Id</td>
                <td>@r.Name</td>
            </tr>
        }
    </tbody>
</table>
